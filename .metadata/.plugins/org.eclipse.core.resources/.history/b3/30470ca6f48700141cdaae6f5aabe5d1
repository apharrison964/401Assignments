
public class Tt {
	
	static int x = 0;

	public static void main(String[] args){
		
		for (int i = 0; i < 2; i++){
			x = i;
		}
		
		System.out.println(x);
		
		if(methodIMade() == true){
			System.out.println("True");
		}else{
			System.out.println("False");
		}
	
	}
	
	public static boolean methodIMade() {
		
		if(x < 0){
			return true;
		}
		return false;
	}
}
